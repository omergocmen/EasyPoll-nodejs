                  denemee=async(text,a)=>
{
    console.log(a.parentElement.parentElement);
    let mhtml=''; 
    let comp=a.parentElement.previousElementSibling;
    while(comp!=null)
    {
        console.log(comp.firstChild.className);
        if(comp.firstChild.className==="fas fa-times")
        {
            
            mhtml='<td><input name="votes" value="61cf4651fc220ec014b5cf17" class="form-check-input" type="checkbox" ></td>'+mhtml
        }
        else if(comp.firstChild.className==="fas fa-check")
        {
            mhtml='<td><input name="votes" value="61cf4651fc220ec014b5cf17" class="form-check-input" type="checkbox"  checked></td>'+mhtml;
        }
        comp=comp.previousElementSibling;   
    }
    
    mhtml+='<td><input name="votes" value="asdsa"  class="form-control" type="text"  id=""></td>';
    a.parentElement.parentElement.innerHTML=mhtml;
    
}






